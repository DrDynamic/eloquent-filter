{

    "name": "mehdi-fathi/eloquent-filter",
    "description": "Allows Dynamic filtering Eloquent Models easily by query string",
    "keywords": [
        "laravel",
        "eloquent",
        "eloquent filter",
        "model",
        "search",
        "query",
        "filter"
    ],
    "license": "MIT",
    "type": "lib",
    "require": {
        "php": "^7.1",
        "illuminate/support": "~5.5.0|~5.6.0|~5.7.0|~5.8.0",
        "illuminate/container": "~5.5.0|~5.6.0|~5.7.0|~5.8.0",
        "illuminate/database": "~5.5.0|~5.6.0|~5.7.0|~5.8.0",
        "illuminate/pagination": "~5.5.0|~5.6.0|~5.7.0|~5.8.0",
        "fzaninotto/faker": "^1.8"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.0|^7.0",
        "orchestra/testbench": "3.5.*|3.6.*",
        "mockery/mockery": "^0.9.5",
        "composer/composer": "^1.8"
    },
    "autoload": {
        "psr-4": {
            "eloquentFilter\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests"
        },
        "classmap": [
            "tests/TestCase.php"
        ]
    },
    "scripts": {
        "test": "vendor/bin/phpunit --coverage-text"
    }
}
