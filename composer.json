{

    "name": "mehdi-fathi/eloquent-filter",
    "description": "The Eloquent filter is a package for filter data of models by the query string. Easy to use and fully dynamic.",
    "keywords": [
        "laravel",
        "laravel tools",
        "eloquent",
        "eloquent filter",
        "Eloquent Filter",
        "laravel query filter",
        "laravel filter",
        "query-filter",
        "dynamic-filters",
        "search",
        "query",
        "filter",
        "laravel filter multiple parameters"
    ],
    "homepage": "https://github.com/mehdi-fathi/eloquent-filter",
    "license": "MIT",
    "type": "lib",
    "authors": [
        {
            "name": "Mehdi Fathi",
            "email": "mehdifathi.developer@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.1",
        "illuminate/support": "5.5.*|5.6.*|5.7.*|5.8.*|^6.0|^6.1|^7.0",
        "illuminate/container": "5.5.*|5.6.*|5.7.*|5.8.*|^6.0|^6.1|^7.0",
        "illuminate/database": "5.5.*|5.6.*|5.7.*|5.8.*|^6.0|^6.1|^7.0",
        "illuminate/pagination": "5.5.*|5.6.*|5.7.*|5.8.*|^6.0|^6.1|^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "6.*|7.0.*|7.5|8.3|8.5",
        "orchestra/testbench": "3.5.*|3.6.*|3.7.*|3.8.*|4.*|4.*|5.*",
        "orchestra/database": "v3.5.0|v3.6.0|v3.7.0|v3.8.0|v4.0.0|v5.0.0|v5.0.0",
        "mockery/mockery": "1.3.*|1.3.*|1.3.*|1.4.x-dev|1.4.x-dev",
        "fzaninotto/faker": "^1.9"
    },
    "autoload": {
        "psr-4": {
            "eloquentFilter\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests"
        },
        "classmap": [
            "tests/TestCase.php"
        ]
    },
    "scripts": {
        "test": "vendor/bin/phpunit --coverage-text"
    }
}
